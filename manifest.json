{
    "manifest_version": 2,
    "name": "Website Time Monitoring",
    "version": "1.0",
    "description": "Tracks time spent on websites",
    "permissions": [
    "tabs",
    "storage",
    "idle"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "popup.html"
    }
  }
  